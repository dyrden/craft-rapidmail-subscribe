{
    "name": "pausch/craft-rapidmail-subscribe",
    "description": "Craft 3.x plugin to subscribe users to a RapidMail mailing list",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "rapidmail-subscribe"
    ],
    "support": {
        "docs": "https://github.com/dyrden/craft-rapidmail-subscribe/blob/master/README.md",
        "issues": "https://github.com/dyrden/craft-rapidmail-subscribe/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Schkade",
            "homepage": "https://github.com/dyrden"
        }
    ],
    "require": {
        "php": "^7.3",
        "craftcms/cms": "^3.0.0",
        "rapidmail/rapidmail-apiv3-client-php": "^1.0"
    },
    "autoload": {
        "psr-4": {
          "pausch\\rapidmailsubscribe\\": "src/"
        }
    },
    "extra": {
        "name": "Rapidmail Subscribe",
        "handle": "rapidmail-subscribe",
        "developer": "Paul Schkade",
        "developerUrl": "http://www.paulschkade.net",
        "documentationUrl": "https://github.com/dyrden/craft-rapidmail-subscribe/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/dyrden/craft-rapidmail-subscribe/master/CHANGELOG.md",
        "components": {
            "rapidmailSubscribeService": "pausch\\rapidmailsubscribe\\services\\RapidmailSubscribeService"
        },
        "class": "pausch\\rapidmailsubscribe\\Rapidmailsubscribe"
    }
}
